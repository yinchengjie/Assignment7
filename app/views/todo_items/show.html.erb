<h1><%= @todo_list.todo_items.find(params[:id]).task_title %></h1>
<div class="todoItem">
  <%= content_tag(:span, "Due on: #{@todo_list.todo_items.find(params[:id]).due_date}") %>
  <br />
  <%= content_tag(:span, "Status: #{@todo_list.todo_items.find(params[:id]).done?}") %>
</div>
<p/>


<%= link_to 'Back to Todo List', todo_list_path(@todo_list) %> |
<%= link_to 'Edit Todo Item', edit_todo_list_todo_item_path(@todo_list, @todo_list.todo_items.find(params[:id])) %> |
<%= link_to 'Delete Todo Item', @todo_item, method: :delete, data: { confirm: 'Are you sure?' } %> |

