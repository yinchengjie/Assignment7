<p id="notice"><%= notice %></p>

<h1>Todo Lists</h1>

<% @todo_lists.each do |todo_list| %>
    <div class="todoList">
      <span class="due_date" style="background:<%= get_color todo_list %>"><%= todo_list.list_due_date %></span>
      <%= link_to "#{todo_list.list_name}", todo_list %>
    </div>
<% end %>

<%= will_paginate @todo_list %>

<br>

<%= link_to 'New Todo list', new_todo_list_path %>
